    "istanbul-coveralls": "1.0.3",
    "less": "2.6.1",
    "lerna": "nolanlawson/lerna#add-jsnext-main-built-3",
    "lie": "3.0.4",
    "mkdirp": "0.5.1",
    "mocha": "2.4.5",
    "mockery": "1.6.2",
    "ncp": "2.0.0",
    "pouchdb-express-router": "0.0.8",
    "pouchdb-server": "1.1.1",
    "replace": "0.3.0",
    "rimraf": "2.5.2",
    "rollup": "0.26.2",
    "rollup-plugin-node-resolve": "1.5.0",
